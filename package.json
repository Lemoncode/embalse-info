{
  "name": "embalse-info",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*",
    "integrations/*",
    "functions",
    "front"
  ],
  "packageManager": "npm@10.0.0",
  "scripts": {
    "prescripts": "npm run start:db -w @embalse-info/db",
    "scripts": "tsx ./run-scripts",
    "prestart": "turbo build",
    "start": "npm run scripts -- start",
    "start:console-runners": "npm run scripts -- console-runners",
    "build": "turbo build",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "dotenv": "^17.2.3",
    "npm-run-all2": "^8.0.4",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.3",
    "turbo": "^2.5.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "cheerio": "^1.1.2",
    "downshift": "^9.2.0",
    "playwright": "^1.54.2"
  }
}
