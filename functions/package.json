{
  "name": "functions",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "prestart": "run-p clean type-check build",
    "start": "func start",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit --preserveWatchOutput",
