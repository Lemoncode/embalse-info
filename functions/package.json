    "watch": "tsc -w"
  },
  "dependencies": {
    "@azure/functions": "^4.0.0",
    "@embalse-info/db": "*"
  },
  "devDependencies": {
    "@types/node": "^20.x"
  },
  "main": "dist/index.js"
}
