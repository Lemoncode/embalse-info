{
  "name": "@embalse-info/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./dist/*"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prestart:db": "docker compose down --remove-orphans",
    "start:db": "docker compose up -d",
    "type-check": "tsc --noEmit --preserveWatchOutput"
  },
  "dependencies": {
    "mongodb": "^6.19.0"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  }
}