{
  "name": "@embalse-info/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./dist/*"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "postinstall": "node setup.js",
    "build": "run-p clean type-check build:db",
    "build:db": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "prestart:db": "docker compose down --remove-orphans",
    "start:db": "docker compose up -d",
    "start:console-runners": "tsx --require dotenv/config --watch src/console-runners/index.ts",
    "type-check": "tsc --noEmit --preserveWatchOutput"
  },
  "dependencies": {
    "arcgis": "*",
    "db-model": "*",
    "mongodb": "^6.19.0",
    "scraping-cuenca-cantabrico": "*",
    "scraping-cuenca-catalana": "*",
    "scraping-cuenca-duero": "*",
    "scraping-cuenca-guadalquivir": "*",
    "scraping-cuenca-jucar": "*",
    "scraping-cuenca-mediterranea": "*"
  },
  "devDependencies": {
    "@types/prompts": "^2.4.9",
    "prompts": "^2.4.2"
  }
}
